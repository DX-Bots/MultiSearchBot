{
    "name": "A Multi Search Bot by @PredatorHackerzZ",
    "description": "A Simple Telegram Inline Multi Search Bot in Pyrogram by @PredatorHackerzZ",
    "keywords": [
        "telegram",
        "inline",
        "torrent",
        "youtube",
        "google",
        "playstore",
        "pypi",
        "bot",
        "image"
    ],
    "repository": "https://github.com/PredatorHackerzZ/MultiSearchBot",
    "website": "https://telegram.dog/TheTeleRoid",
    "success_url": "https://github.com/PredatorHackerzZ/MultiSearchBot",
    "env": {
        "API_ID": {
            "description": "Get this value from my.telegram.org"
        },
        "API_HASH": {
            "description": "Get this value from my.telegram.org"
        },
        "SESSION_NAME": {
            "description": "Any Bot Session Name. You can keep default!",
            "value": "MultiSearchBot"
        },
        "BOT_TOKEN": {
            "description": "Your Telegram Bot Token by @BotFather"
        },
        "MAX_INLINE_RESULTS": {
            "description": "Max Telegram Inline Search Result. Keep Default!",
            "value": "50",
            "required": false
        }
    },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    }
}
